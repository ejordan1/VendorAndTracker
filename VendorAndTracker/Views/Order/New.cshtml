@{
  Layout = "_Layout";
}

<h1>Add a new order to @Model._name of id @Model._id</h1>

<form action="/vendor/@Model._id/new" method="post">
  <input id="vendorId" name="vendorId" type="hidden" value="@Model._id">
  <label for="orderName">Order Name</label>
  <input id="orderName" name="orderName" type="text">
  <button type='submit'>Add Order</button>
</form>