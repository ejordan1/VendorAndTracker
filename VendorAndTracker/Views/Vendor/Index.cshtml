@{
  Layout = "_Layout";
}

@using VendorAndTracker.Models;

<h1>Categories</h1>

@if (@Model.Count == 0)
{
  <h3>No vendors have been added yet!</h3>
} else {
  <h3>Some vendors have been added!</h3>
}
  @foreach (Vendor vendor in Model)
  {
    Console.WriteLine("Vendor each");
    <h3><a href='/vendor/@vendor._id'>@vendor._name</a></h3>
  }



<p><a href='/'>Back home</a></p>