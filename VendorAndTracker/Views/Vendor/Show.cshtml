@{
  Layout = "_Layout";
}

@using VendorAndTracker.Models;
<!-- cannot use Order here because it does not know how to read this,
that is why they used dictionary -->

<h3>Here are all the orders in this category:</h3>

<ol>
@foreach (Order order in @Model["orders"])
{
  <li><a href='/vendor/@Model["id"]/@order._id'>@order._name</a></li>
}
</ol>

<p><a href='/vendor/@Model["id"]/new'>Add a new item</a></p>
<p><a href='/vendor'>Return to category list</a></p>
<p><a href='/'>Return to Main Page</a></p>